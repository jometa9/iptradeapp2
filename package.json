{
  "name": "iptradeapp",
  "private": true,
  "version": "1.2.3",
  "description": "Professional Trading Platform with Copy Trading Capabilities",
  "author": "IPTRADE",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:server\" \"npm run dev:neutralino\"",
    "dev:frontend": "vite --port 31000",
    "dev:server": "node server/src/dev.js",
    "dev:neutralino": "neu run",
    "build": "npm run build:all",
    "build:frontend": "vite build",
    "build:server": "cd server && npm install && npm run build",
    "build:neutralino": "neu build",
    "build:all": "rimraf dist resources/dist && npm run build:frontend && npm run build:server && npm run build:resources && npm run build:neutralino",
    "build:resources": "node scripts/prepare-resources.js",
    "build:windows": "npm run build:all && neu build --release",
    "preview": "vite preview",
    "release": "npm run build:windows",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write . && eslint . --fix"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "lucide-react": "^0.511.0",
    "node-fetch": "^3.3.2",
    "radix-ui": "^1.4.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.57.0",
    "tailwind-merge": "^3.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@tailwindcss/postcss": "^4.1.8",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.5",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.8",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  },
  "build": {
    "appId": "com.iptrade.app",
    "productName": "IPTRADE",
    "copyright": "Copyright Â© 2024 IPTRADE",
    "directories": {
      "output": "release",
      "buildResources": "resources"
    },
    "files": [
      "dist/**/*",
      "resources/**/*"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "public/iconShadow025.png"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "IPTRADE",
      "include": "installer.nsh",
      "installerIcon": "public/iconShadow025.png",
      "uninstallerIcon": "public/iconShadow025.png",
      "installerHeaderIcon": "public/iconShadow025.png",
      "artifactName": "IPTRADE-Setup-${version}.${ext}"
    },
    "protocols": {
      "name": "IPTRADE Protocol",
      "schemes": [
        "iptrade"
      ]
    }
  }
}
